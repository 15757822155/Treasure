<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.zhuoxin.hunttreasure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name="com.zhuoxin.hunttreasure.treasure.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.zhuoxin.hunttreasure.user.login.LoginActivity">
        </activity>
        <activity android:name="com.zhuoxin.hunttreasure.user.register.RegisterActivity">
        </activity>
        <activity android:name="com.zhuoxin.hunttreasure.treasure.HomeActivity">
        </activity>
    </application>

</manifest>